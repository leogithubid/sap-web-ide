{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ui.demo",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
    "rootView": {
			"viewName": "sap.ui.demo.view.App",
			"type": "XML",
			"id": "idApp"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.demo.view",
        		"controlId": "splitApp",
				"async": true
			},
      "routes": [
        {
          "name" : "master",
          "pattern" : "",
          "target" : ["home","blank"]
        },
        {
          "name" : "detail",
          "pattern": "detail",
          "target" : ["detail","blank"]
        }
      ],
      "targets": {
        "home" : {
          "viewName": "viewOne",
          "controlAggregation": "masterPages"
        },
        "blank" : {
          "viewName": "blank",
          "controlAggregation": "detailPages"
        },
        "detail":{
          "viewName": "viewTwo",
          "controlAggregation": "detailPages"
        }
      }
    },
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mockData/fruits.json"
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.i18n.i18n",
					"supportedLocales": [""],
					"fallbackLocale": ""
				}
			}
		}
	}
}